{
	"name": "puppeteer-jest",
	"version": "1.0.0",
	"description": "template for automation using puppeteer and jest",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"stg": "cross-env NODE_ENV=stg ./node_modules/.bin/jest --runInBand --no-color",
		"stg-mobi": "cross-env NODE_ENV=stg MOBI=true ./node_modules/.bin/jest",
		"prd": "cross-env NODE_ENV=prd ./node_modules/.bin/jest --runInBand --no-color",
		"prd-mobi": "cross-env NODE_ENV=prd MOBI=true ./node_modules/.bin/jest",
		"stg-headless": "cross-env NODE_ENV=stg HEADLESS=true ./node_modules/.bin/jest --runInBand --no-color",
		"stg-mobi-headless": "cross-env NODE_ENV=stg HEADLESS=true MOBI=true ./node_modules/.bin/jest",
		"prd-headless": "cross-env NODE_ENV=prd HEADLESS=true ./node_modules/.bin/jest --runInBand --no-color",
		"prd-mobi-headless": "cross-env NODE_ENV=prd HEADLESS=true MOBI=true ./node_modules/.bin/jest"
	},
	"keywords": [],
	"author": "Harsha Vagh",
	"license": "ISC",
	"jest": {
		"verbose": true,
		"setupTestFrameworkScriptFile": "<rootDir>/setUp_tearDown/jest.setup.js",
		"reporters": [
			"default",
			"jest-junit",
			[
				"./node_modules/jest-html-reporter",
				{
					"pageTitle": "Test Report"
				}
			]
		]
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-jest": "^26.0.1",
		"babel-preset-env": "^1.7.0",
		"cross-env": "^6.0.3",
		"faker": "^4.1.0",
		"jest": "^26.0.1",
		"jest-circus": "^26.0.1",
		"jest-cli": "^26.0.1",
		"jest-html-reporter": "^3.1.3",
		"jest-junit": "^10.0.0",
		"puppeteer": "^3.3.0"
	},
	"dependencies": {
		"@babel/preset-env": "^7.10.2",
		"core-js": "^3.6.5",
		"fs": "0.0.1-security",
		"jest-puppeteer": "^4.4.0",
		"request": "^2.88.2",
		"request-promise": "^4.2.5",
		"request-promise-native": "^1.0.8"
	}
}
